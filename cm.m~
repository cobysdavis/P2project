function [xcm1,ycm1,xcm2,ycm2] = cm(intensity,closest,matrixDim)

sumx1=0;
sumy1=0;
sumx2=0;
sumy2=0;
sumt1=
for i=1:matrixDim
    for j=1:matrixDim
        if closest(i,j)==1
            sumx1=sumx1+intensity(i,j)*i;
            sumy1=sumy1+intensity(i,j)*j;
        elseif closest(i,j)==2
            sumx2=sumx2+intensity(i,j)*i;
            sumy2=sumy2+intensity(i,j)*j;
        end
    end
end

xcm1=floor(sumx1/sumt);
ycm=floor(sumy/sumt);
end

